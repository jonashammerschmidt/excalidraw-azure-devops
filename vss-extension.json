{
  "manifestVersion": 1,
  "id": "excalidraw-dev",
  "publisher": "JonasHammerschmidt",
  "version": "0.0.70",
  "name": "Excalidraw (Dev)",
  "description": "View and edit Excalidraw drawings in Azure DevOps",
  "public": false,
  "icons": {
    "small": "images/logos/logo_small.png",
    "branding": "images/logos/logo_branding.png",
    "default": "images/logos/logo_default.png"
  },
  "branding": {
    "color": "#6965db",
    "theme": "light"
  },
  "tags": [
    "excalidraw",
    "draw",
    "sketch",
    "diagrams",
    "whiteboard",
    "visualization",
    "collaboration",
    "planning",
    "design",
    "workflow",
    "documentation",
    "brainstorming",
    "drawing",
    "ui/ux",
    "architecture"
  ],
  "categories": [
    "Azure Boards"
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/jonashammerschmidt/excalidraw-azure-devops"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "drawings",
      "type": "ms.vss-web.hub",
      "targets": [
        "ms.vss-web.home-hub-group"
      ],
      "properties": {
        "name": "Drawings",
        "uri": "index.html",
        "icon": {
          "light": "asset://images/logos/logo_navbar_light_theme.png",
          "dark": "asset://images/logos/logo_navbar_dark_theme.png"
        }
      }
    },
    {
      "id": "drawing-name-form",
      "type": "ms.vss-web.external-content",
      "description": "Dialog content with input",
      "targets": [],
      "properties": {
        "uri": "drawing-name-form.html"
      }
    }
  ],
  "files": [
    {
      "path": "dist/excalidraw-azure-devops/browser",
      "addressable": true,
      "packagePath": "/"
    },
    {
      "path": "src/native",
      "addressable": true,
      "packagePath": "/"
    },
    {
      "path": "images/logos",
      "addressable": true
    },
    {
      "path": "node_modules/azure-devops-extension-sdk",
      "addressable": true,
      "packagePath": "lib"
    }
  ]
}