{
    "manifestVersion": 1,
    "id": "excalidraw-dev",
    "publisher": "JonasHammerschmidt",
    "version": "0.0.1",
    "name": "Excalidraw (Dev)",
    "description": "View and edit Excalidraw drawings in Azure DevOps",
    "public": false,
    "categories": ["Azure Boards"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "drawings",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Drawings",
                "uri": "drawings.html"
            }
        }
    ],
    "files": [
        {
            "path": "drawings.html",
            "addressable": true
        },
        {
            "path": "node_modules/azure-devops-extension-sdk",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}